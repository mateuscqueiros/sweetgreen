{
  "compilerOptions": {
    "target": "es2016",

    "module": "commonjs",

    "rootDir": "src/assets/scripts/ts",
    "outDir": "src/assets/scripts/js",

    "noEmitOnError": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "removeComments": true
  }
}
